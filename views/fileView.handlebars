<div class="file-page">
	<div class="top-bar">
		<div class="inner">
			<div class="container">
				<img class="logo" src="http://thunderbayurc.com/images/logo.png">
				<p>URC Thunder Bay</p>
			</div>
			<div class="container">
				<a class="sign-out" href="{{path}}/logout">{{user.userName}} <i class="fa fa-sign-out"></i></a>
			</div>
		</div>
	</div>
	<div class="main">
		<div class="inner">
			<div class="content">
				<div class="toolbar">
					<div class="breadcrumbs">
						<a href="#">/</a>
						<i class="fa fa-angle-right"></i>
						<a href="#">sub folder</a>
						<i class="fa fa-angle-right"></i>
						<a href="#">inner folder</a>
					</div>
					<div class="tool-container">
						<i class="fa fa-upload fa-2x fa-fw"></i>
						<span class="fa-stack">
							<i class="fa fa-folder-o fa-stack-2x fa-fw"></i>
							<i class="fa fa-plus-circle fa-stack-1x"></i>
						</span>
						<i class="fa fa-trash-o fa-2x fa-fw"></i>
					</div>
				</div>
				<div class="break"></div>
				<table class="file-table">
					<thead>
						<tr>
							<th class="shrink text-center"><i class="fa fa-circle-o"></i></th>
							<th class="indent">Filename</th>
							<th class="shrink">Created By</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td><i class="fa fa-circle-o"></i></td>
							<td class="description-td">
								<i class="fa fa-folder-o fa-fw"></i>
								<div class="text">
									<p>Somefolder</p>
									<p class="sub">created 2 days ago</p>
								</div>
							</td>
							<td>admin</td>
						</tr>
						<tr>
							<td><i class="fa fa-circle-o"></i></td>
							<td class="description-td">
								<i class="fa fa-file-text-o fa-fw"></i>
								<div class="text">
									<p>Somefile.pdf</p>
									<p class="sub">uploaded 2 days ago</p>
								</div>
							</td>
							<td>admin</td>
						</tr>
						<tr>
							<td><i class="fa fa-circle-o"></i></td>
							<td class="description-td">
								<i class="fa fa-file-text-o fa-fw"></i>
								<div class="text">
									<p>another.pdf</p>
									<p class="sub">uploaded 5 weeks ago</p>
								</div>
							</td>
							<td>craig.dykstra</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>